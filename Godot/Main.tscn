script/source = "extends KinematicBody2D

var anim_player
var label
var win_label
var tween

const SPEED = 200
const MAX_VELOCITY = 200
const JUMP = 275
const GRAVITY = 1000

var velocity = Vector2()
var facing_right = false

var death_count = 0

# Called when the node enters the scene tree for the first time.
func _ready():
	anim_player =  $AnimatedSprite
	label = get_parent().get_node(\"deathcount\")
	win_label = get_parent().get_node(\"win\")
	win_label.set_visible(false)
	
	tween = get_parent().get_node(\"Tween\")
	
func _physics_process(delta):
	label.set_text(str(death_count))
	var dir = 0
	var onFloor = is_on_floor();
	
	if velocity.x > MAX_VELOCITY:
		var new_speed = velocity.normalized()
		new_speed *= MAX_VELOCITY
		velocity.x = new_speed
		
	if onFloor and velocity.y > MAX_VELOCITY:
		velocity.y = MAX_VELOCITY
	else:
		velocity.y += delta * GRAVITY

	if Input.is_action_pressed(\"ui_left\"):
		dir -=1
		velocity.x = -SPEED
	elif Input.is_action_pressed(\"ui_right\"):
		dir +=1
		velocity.x = SPEED
	else:
		velocity.x = 0
	
	if onFloor and Input.is_action_pressed(\"ui_up\"):
		velocity.y = -JUMP

	if facing_right and dir > 0:
		flipSprite()
	if !facing_right and dir < 0:
		flipSprite()
		
	if onFloor:
		if dir == 0:
			PlayAnim('idle')
		else:
			PlayAnim('run')
	else:
		PlayAnim('jump')
		
	move_and_slide(velocity, Vector2(0, -1))
	
	if position.y >= 500:
		position.x = -55
		position.y = 8
		death_count += 1
		
	if position.x >= 925:
		win_label.set_visible(true)
		tween.interpolate_callback(self, 10, \"ReloadCurrentScene\")
		tween.start()

func flipSprite():
	facing_right = !facing_right
	anim_player.flip_h = !anim_player.flip_h

func PlayAnim(name):
	if anim_player.is_playing() and anim_player.animation == name:
		return
	anim_player.play(name)

func ReloadCurrentScene():
	get_tree().reload_current_scene()
"

